{
    "name" : "profile",
    "description":"Vue.JS Plugin for Soss Router",
    "author":"Supun Dissanayake",
    "version" : "0.1",
    "gitUrl" : "",
    "resources": {
        "files":[
            {
                "type" : "mainScript",
                "location" : "script.js"
            }
        ]
    },
    "serviceHandler" : {
        "file": "service.php",
        "class": "ProfileService",
        "methods" : {
            "Search" : {
                "method" : "GET",
                "parameters": ["q"]
            },
            "SearchV1":{
                "method" : "GET",
                "parameters": ["column","value"]
            },
            "ByID" :{
                "method" : "GET",
                "parameters": ["id"]
            },
            "SupplierData":{
                "method" : "GET",
                "parameters": []
            },
            "q" : {
                "method" : "POST"
            },
            "ChangeStatus":{
                "method" : "POST"
            }
        }
    
    }
    
}