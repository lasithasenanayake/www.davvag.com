        "productsvr": {
            "type":"service",
            "location":"services"
        },
        "frmproduct-list": {
            "type":"component",
            "location":"components"
        },
        "frmproduct": {
            "type":"component",
            "location":"components"
        },
        "newproduct": {
            "type":"component",
            "location":"components"
        },
        "partial-cart":{
            "type":"component",
            "location":"components"
        },
        "partial-cart-checkout":{
            "type":"component",
            "location":"components"
        }
    },
    "description":{
        "title": "Davvag Market Place",
        "author":"Davvag",
        "version":"0.5",
        "icon":"appicon.png"
    },
    "tags":["showindock"],
    "configuration":{
        "webdock":{
            "startupComponent" : "frmproduct-list",
            "onLoad": ["productsvr"],
